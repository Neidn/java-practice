package Ans200130;

public class Card {
	private String cardVal;

	public String getCardVal() {
		return cardVal;
	}

	// 디폴트 생성자
	public Card() {
		int suit = (int) (Math.random() * CardUtil.SUIT.length);
	}

	public Card(String s) {
		this.cardVal = s;
	}

	public Card(Card c) {
		this(c.getCardVal());
	}

	@Override
	public String toString() {
		return "[" + cardVal + "]";
	}

}
